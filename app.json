{
  "expo": {
    "name": "WspinApp",
    "slug": "WspinApp",
    "scheme": "wspinapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "./src/plugins/android-manifest.plugin.js",
      "./src/plugins/with-android-queries.js",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Outfit-Black.ttf",
            "./src/assets/fonts/Outfit-Bold.ttf",
            "./src/assets/fonts/Outfit-ExtraBold.ttf",
            "./src/assets/fonts/Outfit-ExtraLight.ttf",
            "./src/assets/fonts/Outfit-Light.ttf",
            "./src/assets/fonts/Outfit-Medium.ttf",
            "./src/assets/fonts/Outfit-Regular.ttf",
            "./src/assets/fonts/Outfit-SemiBold.ttf",
            "./src/assets/fonts/Outfit-Thin.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyCRqqeDJKrGyvyn88GiWdpC-6MU52IwJL8"
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "transit",
          "truckmap",
          "waze",
          "yandexnavi",
          "moovit",
          "yandextaxi",
          "yandexmaps",
          "kakaomap",
          "szn-mapy",
          "mapsme",
          "osmandmaps",
          "gett",
          "nmap",
          "dgis",
          "lftgpas",
          "mailto",
          "tel"
        ]
      },
      "bundleIdentifier": "qweq"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDr21ZqwVKMLEES_968W9Ivbad8lHmcu3s"
        }
      },
      "permissions": ["android.permission.INTERNET"],
      "package": "com.wspinapp"
    }
  }
}
